{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "strict": true,
      "jsx": "react-jsx", 
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noUnusedLocals": false,
      "allowJs": true,
      "moduleResolution": "node",
      "baseUrl": "./",
      "paths": {
        "@/*": ["src/*"]
      }
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  